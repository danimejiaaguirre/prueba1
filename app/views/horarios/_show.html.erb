<%- model_class = Horario -%>
<div class="page-header">
  <h1 class='titulos'>Horario</h1>
</div>

<div class='horario'>
  <p><span class='mostrar_tit'>Id: </span><%= @horario.id %></p>
  <p><span class='mostrar_tit'>Materia: </span> <%= @horario.materia_nombre %></p>
  <p><span class='mostrar_tit'>Maetro: </span> <%= @horario.maestro_nombre %></p>
  <p><span class='mostrar_tit'>Curso: </span><%= @horario.curso_nombre %></p>
  <p><span class='mostrar_tit'>Dia: </span><%= @horario.dia %></p>
  <p><span class='mostrar_tit'>Hora: </span><%= @horario.hora %></p>
  <p><span class='mostrar_tit'>Aula: </span><%= @horario.aula %></p>
</div>
